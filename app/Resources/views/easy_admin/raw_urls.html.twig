{% if view == 'show' %}
    <div class="raw_urls">
        {{ value|length }} urls
        {% if value is not empty %}
            <table>
                <thead>
                <tr>
                    {% for key in value.0|keys %}
                        <th>{{ key }}</th>
                    {% endfor %}
                </tr>
                </thead>
                <tbody>
                {% for link in value %}
                    <tr>
                        {% for val in link %}
                            <td>{{ val }}</td>
                        {% endfor %}
                    </tr>
                {% endfor %}
                </tbody>
            </table>
        {% endif %}
    </div>
{% else %}
    {{ value|length }} raw urls
{% endif %}
